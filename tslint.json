{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "member-ordering": false,
    "max-line-length": false,
    "no-bitwise": false,
    "no-namespace": false,
    "no-empty-interface": false,
    "no-empty": false
  },
  "rulesDirectory": []
}